{
  "name": "pitch-it",
  "version": "0.8.0",
  "description": "WebRTC based communication tool",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/J-Pi/pitch-it.git"
  },
  "scripts": {
    "start": "node ./server/run.js",
    "clean": "node ./scripts/clean.js",
    "copy": "node ./scripts/copy.js",
    "build": "npm run clean && npm run copy && webpack --config ./webpack.config.js",
    "dev": "webpack-dev-server --config ./webpack.dev.config.js",
    "test": "mocha ./test"
  },
  "dependencies": {
    "express": "4.14.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "ws": "1.1.1"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "css-loader": "0.25.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "3.0.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
  }
}
